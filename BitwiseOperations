 import java.io.*;

class BitwiseOperations {
    public static void main(String[] args) {
        int num1 = 2;
        int num2 = 5;
        
        System.out.println(num1 ^ num2);  // Output: 1
        System.out.println(num1 > num2 & num2 < num1);  // Output: false
        System.out.println(num1 > num2 | num1 < num2);  // Output: true
    }
}
